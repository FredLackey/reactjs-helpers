{
  "name"       : "reactjs-helpers",
  "version"    : "0.0.1",
  "description": "Helpful utilities and scripts to make ReactJS projects more legible and easier for the next developer to take over.",
  "main"       : "index.js",
  "type"       : "module",
  "scripts"    : {
    "test"      : "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:trace": "node --trace-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint"      : "eslint --ext .js,.jsx .",
    "lint:fix"  : "eslint --fix --ext .js,.jsx ."
  },
  "keywords": [
    "react",
    "reactjs",
    "utilities",
    "helpers",
    "scripts",
    "development",
    "legibility",
    "maintenance",
    "code quality",
    "eslint",
    "react-hooks",
    "jest",
    "testing",
    "project management",
    "developer tools"
  ],
  "author": {
    "name" : "Fred Lackey",
    "email": "fred.lackey@gmail.com",
    "url"  : "http://fredlackey.com"
  },
  "repository": "github:FredLackey/reactjs-helpers",
  "bugs"      : {
    "url"  : "https://github.com/FredLackey/reactjs-helpers/issues",
    "email": "fred.lackey@gmail.com"
  },
  "license"        : "Apache-2.0",
  "devDependencies": {
    "@babel/core"              : "^7.23.7",
    "@babel/preset-env"        : "^7.23.7",
    "babel-jest"               : "^29.7.0",
    "eslint"                   : "^8.56.0",
    "eslint-plugin-react"      : "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest"                     : "^29.7.0",
    "react"                    : "^18.2.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "readline"    : "^1.3.0",
    "rimraf"      : "^5.0.5",
    "uuid"        : "^9.0.1"
  }
}
